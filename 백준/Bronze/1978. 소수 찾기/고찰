while(True):
    N = int(input())
    if (N==0):
        break
    count=0
    for i in range (N+1,2*N+1):
        condition = True
        if(i==1):
            continue
        if(i==2 or i==3):
            count+=1
            continue
        j=1
        while(j*j<i):
            j+=1
            if(i%j==0):
                condition = False
                break
        if(condition):
            count+=1
    print(count)
